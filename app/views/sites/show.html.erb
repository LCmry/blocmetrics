<%= javascript_include_tag 'sites' %>
<%= javascript_tag do %>
  views = '<%= @site.events.count %>';
  dates = '<% @site.events.each do |event| %>
    <%= event.created_at.to_date %>
  <% end %>';
<% end %>
<h1><%= @site.name %></h1>
Site Id: <%= @site.id %>
<p><%= @site.events.count %></p>
<canvas id="lineChart" width="400" height="400"></canvas>
<%= link_to "Delete Site", [@user, @site], method: :delete %>
<%= link_to "Back", @user %>